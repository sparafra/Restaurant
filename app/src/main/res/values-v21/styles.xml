<resources>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
    <style name="ListViewStyle" parent="Widget.AppCompat.ListView">
        <item name="colorControlActivated">#ffff</item>
        <item name="colorControlNormal">#ffff</item>
        <item name="android:textColor">#ffff</item>
    </style>
    <style name="SwitchTheme" parent="Theme.AppCompat.Light">
        <item name="android:textColorPrimaryDisableOnly">#00838f</item>
        <item name="colorAccent">#148E13</item>
        <item name="android:textColor">@drawable/White</item>

    </style>

</resources>
